  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "nx workspace-generator",
      "skipFiles": ["<node_internals>/**"],
      // "program": "${workspaceFolder}/tools/generators/my-generator/index.ts",
